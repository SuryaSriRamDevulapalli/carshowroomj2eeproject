       <servlet>
    <servlet-name>MahhindraXUV700Servlet</servlet-name>
    <servlet-class>com.servlet.controller.MahhindraXUV700Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CreatedServlet</servlet-name>
        <url-pattern>/CreatedServlet</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>CreatedServletScorpio</servlet-name>
        <url-pattern>/CreatedServletScorpio</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>CreatedServletThar</servlet-name>
